<script>
    new WOW().init();
    $(function () {
        $(window).scroll(function(){
            var scrollTop = $(this).scrollTop();

            if ($(window).scrollTop() > 2945){
                var pos = (scrollTop - 3045) / 2;
                $('.border').css('border-width', 80 - (Math.floor(pos)/2));
            }

        });
    })
</script>